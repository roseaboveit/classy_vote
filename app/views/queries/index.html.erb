<div class="jumbotron">
  <h1>Question Index</h1>
  <div class="text-center">
    <%= link_to "Log out", destroy_user_session_path, :method => :delete %> |
    <%= link_to "Profile Page", "/profile"%> |
    <%= link_to "Index", "/queries" %>
  </div>
</div>

<div class="well">
  <h3>All Questions</h3>
    <div class="well">
      <% @queries.each do |q| %>
      <div>  
        <a class="question" href="/queries/<%= q.id %>"><%= q.question %></a> 
      <% end %>
    </div>
  </div>
</div>


